# editorconfig.org

root = true


[*]
charset = utf-8
end_of_line = lf
indent_size = 4
continuation_indent_size = 8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
charset = utf-8
end_of_line = lf
indent_size = 4
continuation_indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yml]
charset = utf-8
end_of_line = lf
indent_size = 2
continuation_indent_size = 2
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = true

[LICENSE.code.txt]
charset = utf-8
end_of_line = lf
indent_size = 3
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = true
